COMM **********************************************************************************************
COMM   Test: EP command, SQ1_FB_MODE register
COMM **********************************************************************************************
COMM
COMM ==============================================================================================
COMM   Enable Squid1 ADC/DAC clocks reports display
COMM ----------------------------------------------------------------------------------------------
CCPE clk_sq1_adc(0)
CCPE clk_sq1_adc(1)
CCPE clk_sq1_adc(2)
CCPE clk_sq1_adc(3)
CCPE clk_sq1_dac(0)
CCPE clk_sq1_dac(1)
CCPE clk_sq1_dac(2)
CCPE clk_sq1_dac(3)
COMM
COMM ==============================================================================================
COMM   Asynchronous reset activated
COMM ----------------------------------------------------------------------------------------------
WDIS arst_n 0
WAIT 80 ns
COMM
COMM ==============================================================================================
COMM   Asynchronous reset deactivated
COMM ----------------------------------------------------------------------------------------------
WDIS arst_n 1
COMM
COMM ==============================================================================================
COMM   Wait internal reset deactivated
COMM ----------------------------------------------------------------------------------------------
WUDI rst 0
COMM
COMM ==============================================================================================
COMM   Start SQ1_FB_MODE register test
COMM ----------------------------------------------------------------------------------------------
WCMD R-SQ1_FB_MODE-XXXX W     # Command 0
WCMD W-SQ1_FB_MODE-8XXX W     # Command 1
WCMD R-SQ1_FB_MODE-XXXX R     # Command 2
COMM
COMM ==============================================================================================
COMM   Check SQ1_FB_MODE content at start:
COMM   Col(3)->Off, Col(2)->Off, Col(1)->Off, Col(0)->Off
COMM ----------------------------------------------------------------------------------------------
CCMD R-SQ1_FB_MODE-0000 W     # Return Command 0
WCMD W-SQ1_FB_MODE-4XXX R     # Command 3
COMM
COMM ==============================================================================================
COMM   Check error position SPI data out of range detected
COMM ----------------------------------------------------------------------------------------------
CCMD R-Status-1000 W          # Return Command 1
WCMD R-SQ1_FB_MODE-XXXX R     # Command 4
COMM
COMM ==============================================================================================
COMM   Check SQ1_FB_MODE no change
COMM ----------------------------------------------------------------------------------------------
CCMD R-SQ1_FB_MODE-0000 W     # Return Command 2
WCMD W-SQ1_FB_MODE-X8XX R     # Command 5
COMM
COMM ==============================================================================================
COMM   Check error position SPI data out of range detected
COMM ----------------------------------------------------------------------------------------------
CCMD R-Status-1000 W          # Return Command 3
WCMD R-SQ1_FB_MODE-XXXX R     # Command 6
COMM
COMM ==============================================================================================
COMM   Check SQ1_FB_MODE no change
COMM ----------------------------------------------------------------------------------------------
CCMD R-SQ1_FB_MODE-0000 W     # Return Command 4
WCMD W-SQ1_FB_MODE-X4XX R     # Command 7
COMM
COMM ==============================================================================================
COMM   Check error position SPI data out of range detected
COMM ----------------------------------------------------------------------------------------------
CCMD R-Status-1000 W          # Return Command 5
WCMD R-SQ1_FB_MODE-XXXX R     # Command 8
COMM
COMM ==============================================================================================
COMM   Check SQ1_FB_MODE no change
COMM ----------------------------------------------------------------------------------------------
CCMD R-SQ1_FB_MODE-0000 W     # Return Command 6
WCMD W-SQ1_FB_MODE-XX8X R     # Command 9
COMM
COMM ==============================================================================================
COMM   Check error position SPI data out of range detected
COMM ----------------------------------------------------------------------------------------------
CCMD R-Status-1000 W          # Return Command 7
WCMD R-SQ1_FB_MODE-XXXX R     # Command 10
